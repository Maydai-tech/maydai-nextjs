{
  "name": "maydi-admin-app",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "dev:clean": "rm -rf .next node_modules/.cache && next dev",
    "clean": "bash scripts/clean-cache.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=tests/jest-e2e.config.js",
    "test:e2e:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run test:e2e\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "claude:issue": "bash scripts/claude-issue.sh"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.9",
    "airtable": "^0.12.2",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^17.1.0",
    "puppeteer": "^24.11.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.1",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
